//check if array is sorted or not

#include <bits/stdc++.h>
using namespace std;

class Solution{
    public:
    bool isSorted(vector <int> &arr){
        int n = arr.size();
        for(int i = 0; i <n-1; i++){
            if(arr[i] > arr[i+1]){
                return false;
            }
        }
        return true;
    }
};

int main(){
    int n;
    cout << "Size of elements: ";
    cin >> n;
    
    vector <int> arr(n);
    cout << "Enter the elements: " << endl;
    for(int i = 0; i < n; i++){
        cin >> arr[i];
    }
    Solution obj;
    cout << (obj.isSorted(arr)? "true" : "false") << endl;
    return 0;
}

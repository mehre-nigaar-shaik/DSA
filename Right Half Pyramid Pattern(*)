
//Right Half Pyramid Pattern(*)
/* 
     *
     **
     ***
             */

#include <iostream>
using namespace std;

int main(){
    int N = 5;
    for (int i = 1; i <= N; i++) {
        for (int j = 1; j <= i; j++) {
            cout << "*";
        }
        cout << "\n";
    }

    return 0; 
}
